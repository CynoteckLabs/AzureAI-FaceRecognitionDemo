@model Microsoft.ProjectOxford.Face.Contract.Person
@{
    ViewBag.Title = "Details";
}
<h2>Person details</h2>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <label asp-for="PersonId"></label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.PersonId)
        </dd>
        <dt>
            <label asp-for="Name"></label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt>
            <label asp-for="UserData"></label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.UserData)
        </dd>
        <dt>
            <label asp-for="PersistedFaceIds"></label>
        </dt>
        <dd>
            @foreach (var id in Model.PersistedFaceIds)
            {
                @id <br />
            }
        </dd>
    </dl>
</div>
<p>
   
</p>